{
  "require": {
    "craftcms/cms": "3.1.21.1",
    "vlucas/phpdotenv": "^2.4.0",
    "craftcms/redactor": "^2.3",
    "craftcms/aws-s3": "1.2.2",
    "craftcms/contact-form": "2.2.4",
    "rias/craft-contact-form-extensions": "1.1.4",
    "verbb/feed-me": "3.1.16",
    "craftcms/element-api": "^2.5",
    "nystudio107/craft-seomatic": "3.1.47",
    "workingconcept/craft-cloudflare": "0.2.9"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
