{
  "require": {
    "craftcms/cms": "3.1.17.1",
    "vlucas/phpdotenv": "^2.4.0",
    "craftcms/redactor": "^2.3",
    "craftcms/aws-s3": "1.2.1",
    "craftcms/contact-form": "^2.2",
    "rias/craft-contact-form-extensions": "^1.1",
    "verbb/feed-me": "3.1.15",
    "craftcms/element-api": "^2.5",
    "nystudio107/craft-seomatic": "^3.1.45",
    "workingconcept/craft-cloudflare": "^0.2.8"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
