{
  "require": {
    "craftcms/cms": "3.1.3",
    "vlucas/phpdotenv": "^2.4.0",
    "craftcms/redactor": "^2.3",
    "craftcms/aws-s3": "^1.0",
    "craftcms/contact-form": "^2.2",
    "rias/craft-contact-form-extensions": "^1.0",
    "verbb/feed-me": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
